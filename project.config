[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/kejmil01
[access "refs/for/refs/*"]
	read = group user/kejmil01
	push = group user/kejmil01
	label-Code-Review = -2..+2 group user/kejmil01
	label-Verified = -1..+1 group user/kejmil01
	rebase = group user/kejmil01
	removeReviewer = group user/kejmil01
	submit = group user/kejmil01
[access "refs/heads/*"]
	read = group user/kejmil01
	create = group user/kejmil01
	push = +force group user/kejmil01
	pushMerge = group user/kejmil01
[access "refs/tags/*"]
	push = group user/kejmil01
	push = +force group user/kejmil01
